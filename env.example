# OpenAI API Key (required for Realtime API)
OPENAI_API_KEY=your_openai_api_key_here

# Englify Backend API Configuration
# Base URL for your Englify backend API
ENGLIFY_API_URL=https://your-englify-backend.com

# API token for Englify backend
ENGLIFY_API_TOKEN=your_api_token_here

# Bearer token for authenticated requests (get this from user login)
ENGLIFY_BEARER_TOKEN=your_bearer_token_here
